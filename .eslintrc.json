{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "self": "readonly"
    },
    "rules": {
        "no-console": "off"
    },
    "overrides": [
        {
            "files":["src-sw.js"],
            "rules": {
                "no-restricted-globals": "off"
            }
        }
    ]
}
